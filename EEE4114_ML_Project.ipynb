   "source": [
    "from torch.utils.data import TensorDataset, DataLoader\n",
    "\n",
    "# Apply windowing to each split\n",
    "X_win_train, y_win_train = create_windowed_data(X_train, Y_train.values, window_size=400, stride=200)\n",
    "X_win_val, y_win_val = create_windowed_data(X_val, Y_val.values, window_size=400, stride=200)\n",
    "X_win_test, y_win_test = create_windowed_data(X_test, Y_test.values, window_size=400, stride=200)\n",
    "\n",
    "# Convert numpy arrays to torch tensors\n",
    "X_train_tensor = torch.tensor(X_win_train, dtype=torch.float32)\n",
    "y_train_tensor = torch.tensor(y_win_train, dtype=torch.long)\n",
    "\n",
    "X_val_tensor = torch.tensor(X_win_val, dtype=torch.float32)\n",
    "y_val_tensor = torch.tensor(y_win_val, dtype=torch.long)\n",
    "\n",
    "X_test_tensor = torch.tensor(X_win_test, dtype=torch.float32)\n",
    "y_test_tensor = torch.tensor(y_win_test, dtype=torch.long)\n",
    "\n",
    "# Create TensorDatasets\n",
    "train_dataset = TensorDataset(X_train_tensor, y_train_tensor)\n",
    "val_dataset = TensorDataset(X_val_tensor, y_val_tensor)\n",
    "test_dataset = TensorDataset(X_test_tensor, y_test_tensor)\n",
    "\n",
    "# Create DataLoaders\n",
    "train_loader = DataLoader(train_dataset, batch_size=32, shuffle=True)\n",
    "val_loader = DataLoader(val_dataset, batch_size=32, shuffle=False)\n",
    "test_loader = DataLoader(test_dataset, batch_size=32, shuffle=False)"
   ]
