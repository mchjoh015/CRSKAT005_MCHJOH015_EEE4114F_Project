    "class CNN1D(nn.Module):\n",
    "    def __init__(self, input_channels=6, num_classes=6):\n",
    "        super(CNN1D, self).__init__()\n",
    "\n",
    "        self.conv1 = nn.Conv1d(in_channels=3, out_channels=64, kernel_size=5, padding=2)\n",
    "        self.bn1 = nn.BatchNorm1d(64)\n",
    "        self.conv2 = nn.Conv1d(64, 128, kernel_size=5, padding=2)\n",
    "        self.bn2 = nn.BatchNorm1d(128)\n",
    "        self.pool = nn.MaxPool1d(kernel_size=2)\n",
    "\n",
    "        self.dropout = nn.Dropout(0.5)\n",
    "        self.fc1 = nn.Linear(128 * 100, 256)  # 400 -> 200 after pooling → 100 steps left after 2 layers\n",
    "        self.fc2 = nn.Linear(256, num_classes)\n",
    "\n",
    "    def forward(self, x):\n",
    "        # Input: (batch, channels, time) e.g., (32, 6, 400)\n",
    "        x = F.relu(self.bn1(self.conv1(x)))  # (32, 64, 400)\n",
    "        x = self.pool(x)                     # (32, 64, 200)\n",
    "        x = F.relu(self.bn2(self.conv2(x)))  # (32, 128, 200)\n",
    "        x = self.pool(x)                     # (32, 128, 100)\n",
    "\n",
    "        x = x.view(x.size(0), -1)            # Flatten: (32, 128 * 100)\n",
    "        x = self.dropout(F.relu(self.fc1(x)))\n",
    "        x = self.fc2(x)                      # Output logits: (32, num_classes)\n",
    "        return x"
   ]
