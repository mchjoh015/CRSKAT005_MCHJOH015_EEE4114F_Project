    "\n",
    "# Split indices sequentially (no shuffle, to preserve time order)\n",
    "train_indices = range(0, train_end)\n",
    "val_indices = range(train_end, val_end)\n",
    "test_indices = range(val_end, n_samples)\n",
    "\n",
    "# Create datasets based on these indices\n",
    "X_train = X.iloc[train_indices]\n",
    "Y_train = Y.iloc[train_indices]\n",
    "\n",
    "X_val = X.iloc[val_indices]\n",
    "Y_val = Y.iloc[val_indices]\n",
    "\n",
    "X_test = X.iloc[test_indices]\n",
    "Y_test = Y.iloc[test_indices]"
